package project.softplc

@Build(stage=1,script="$(EXTERNAL_MAKE)")
@BuildVariable(name="iec2c", value="$(EXTERNAL_BUILD_DIR)/project/softplc/iecsup/install/bin/iec2c")
@BuildVariable(name="ieclib", value="$(EXTERNAL_BUILD_DIR)/project/softplc/iecsup/install/lib")
@BuildVariable(name="softplc_main", value="$(PROJECT_DIR)/softplc/iecsup/softplc_main.c")
@BuildArtifactPath(cppflags="-I$(EXTERNAL_BUILD_DIR)/project/softplc/iecsup/install/include")
@BuildArtifactPath(cppflags="-Wno-unused-variable -Wno-unused-function")
module iecsup {
}
