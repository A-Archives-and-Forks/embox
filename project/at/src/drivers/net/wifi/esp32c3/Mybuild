package project.at.driver.net.wifi.esp32c3

module esp32c3_emulator {
	source "emulator/esp32c3_emulator.c"

	@IncludeExport(path="wifi/esp32c3")
	source "emulator/esp32c3_emulator.h"

	depends project.at.driver.at.at_emulator
}

module esp32c3_wifi {
  @DefineMacro("CONFIG_ESP32C3_AUTO_INIT=1")
  option string uart_path="/dev/ttyS1"

  source "esp32c3_wifi.c"

	@IncludeExport(path="wifi/esp32c3")
	source "esp32c3_wifi.h"

  depends project.at.driver.net.wifi.wifi_core
}
